        layout (@ 'widget (@ 'x 50 'y 10 'w 700 'h 200)
                  'title (@ 'h 50 'pos 'top)
                  'y-axis (@ 'w 100 'pos 'left)
                  'x-axis (@ 'h 40 'pos 'bottom)
