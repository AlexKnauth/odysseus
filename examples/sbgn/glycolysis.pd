#lang racket

(require "../../sbgn/pd.rkt")

; glycolysis process:

(define ATP 'ATP)
(define ADP 'ADP)
(define H2O 'H2O)
(define Pi (s-chemical Pi))
(define NAD (s-chemical NAD))
(define NADH (s-chemical NADH))
(define H+ (s-chemical H+))

(sbgn/pd
  (process
    (in (s-chemical glucose) ATP)
      [catalyst (enzyme hexokynase)]
    (out (s-chemical glucose6P _) ADP))
  (process
    (in _)
      [catalyst (enzyme glucose-6P_isomerase)]
    (out (s-chemical fructose-6P _)))
  (process
    (in _ ATP)
      [catalyst (enzyme posphofructokinase)]
    (out (s-chemical fructose-1.6P _) ADP))
  (process
    (in _)
      [catalyst (enzyme aldolase)]
    (options
      (out (s-chemical DHA-P _))
      (out (s-chemical GA-3P __))))
  (process<>
    (in _)
      [catalyst (enzyme triase-P_isomerase)]
    (out __))
  (process
    (in __ Pi NAD)
      [catalyst (enzyme GAPDH)]
    (out (s-chemical 1.3_BPG _) NADH H+))
  (process
    (in _ ADP)
      [catalyst (enzyme PGK1)]
    (out (s-chemical 3PG _) ATP))
  (process
    (in _)
      [catalyst (enzyme PG_mutase)]
    (out (s-chemical 2PG _)))
  (process
    (in _)
      [catalyst (enzyme enolase)]
    (out (s-chemical PEP _) H2O))
  (process
    (in _)
      [catalyst (enzyme pyruvate_kinase)]
    (out (s-chemical pyruvate))))
